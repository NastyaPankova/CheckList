networks:
  check_list_network:
    driver: bridge
    name: CheckListNet
services:
  api:
    build:
      context: C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution
      dockerfile: Systems/Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Api
      target: base
    container_name: Api
    depends_on:
      sqlserver:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CONNECTION_STRING_DATABASE: CheckListNet
      CONNECTION_STRING_HOST: sqlserver
      CONNECTION_STRING_PASSWORD: Passw(0)rd
      CONNECTION_STRING_USER: sa
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      check_list_network: {}
    ports:
    - published: 20000
      target: 80
    tty: true
    volumes:
    - C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution\Systems\Api:/app:rw
    - C:\Users\═рёЄ \vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\═рёЄ \AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\═рёЄ \AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\═рёЄ \.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution:/src:rw
  identity:
    build:
      context: C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution
      dockerfile: Systems/Identity/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Identity
      target: base
    container_name: Identity
    depends_on:
      sqlserver:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      IDENTITY_SERVER_CLIENT_ID: CheckListNet
      IDENTITY_SERVER_CLIENT_SECRET: '123456'
      IDENTITY_SERVER_URL: localhost:20002
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: identity:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Identity.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      check_list_network: {}
    ports:
    - published: 20001
      target: 80
    tty: true
    volumes:
    - C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution\Systems\Identity:/app:rw
    - C:\Users\═рёЄ \vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\═рёЄ \AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\═рёЄ \AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\═рёЄ \.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\═рёЄ \source\repos\CheckList\CheckListSolution:/src:rw
  sqlserver:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Passw(0)rd
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      check_list_network: {}
    ports:
    - published: 21433
      target: 1433
